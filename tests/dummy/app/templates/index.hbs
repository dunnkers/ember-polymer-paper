{{#each snippets.snippets as |snippet|}}
  <paper-card heading={{snippet.name}} class="index-element-card">
    <div class="card-content">
      {{component (concat snippet.name '-example')}}
    </div>
    <div class="card-actions">
      {{#link-to snippet.name tabIndex="-1" class="paper-button-link"}}
        <paper-button>Full documentation</paper-button>
      {{/link-to}}
    </div>
  </paper-card>
{{/each}}
