<paper-drawer-panel>
  <paper-header-panel slot="drawer">
    <paper-toolbar>
      <div slot="top">Elements</div>
    </paper-toolbar>
    <paper-menu>
      {{#link-to 'index' tabindex="-1"}}
        <paper-item>All elements</paper-item>
      {{/link-to}}
      {{#each snippets.snippets as |snippet|}}
        {{#link-to snippet.name tabindex="-1"}}
          <paper-item>{{snippet.name}}</paper-item>
        {{/link-to}}
      {{/each}}
    </paper-menu>
  </paper-header-panel>
  <paper-header-panel slot="main">
    <paper-toolbar>
      <span slot="top" class="title">Ember Polymer Paper demo ğŸ¦„âœ¨</span>

      <a slot="top" href="https://github.com/dunnkers/ember-polymer-paper" tabIndex="-1">
        <img src="images/github.png" class="github-logo">
      </a>
    </paper-toolbar>
    <div class="outlet">
      {{outlet}}
    </div>
  </paper-header-panel>
</paper-drawer-panel>
